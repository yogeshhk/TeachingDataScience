
#set dgrid3d 10,10
#set isosample 10
set contour base
set cntrparam linear
set cntrparam levels discrete .2,.5,.8
unset surface
set table 'z.contour.dat'
splot [-2:3][-2:3] \
 'z.model' matrix us ($2/10-2):($1/10-2):3
unset table

plot [-2:3][-2:3] \
 'z.train' us 1:2:3 with points pt 2 lc variable title 'z.train',\
 './z.contour.dat' us 1:2:(abs($3-.5)/.4) with lines lc variable title 'decision boundary'
